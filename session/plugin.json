{
  "name": "session",
  "version": "3.15.0",
  "description": "• v3.15.0: Plan transformation path bug fix, enhanced plan status\n• v3.14.2: Global plan storage, removed session dependency\n• v3.14.1: Work type detection fix\n• v3.14.0: Node.js v24.7.0 compatibility, repository URL fixes\n• v3.13.0: Project context maps system (multi-layered project analysis)\n• v3.12.0: Requirements-based planning workflow, AI-powered task breakdown",
  "author": {
    "name": "AutomateWith.Us",
    "email": "team@automatewith.us"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awudevelop/claude-plugins.git"
  },
  "keywords": [
    "session",
    "snapshot",
    "auto-capture",
    "memory",
    "workflow",
    "productivity",
    "optimization",
    "plan-mode",
    "token-reduction",
    "performance",
    "git-history",
    "repository-context",
    "context-tracking"
  ],
  "category": "productivity",
  "license": "MIT",
  "commands": "./commands",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/session-start.js"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/user-prompt-submit.js"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      },
      {
        "matcher": "NotebookEdit",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/stop.js"
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/session-end.js"
          }
        ]
      }
    ]
  },
  "versionMetadata": {
    "lastUpdateCommit": "e6fa4622989bc17734ca995db3438d24bfe9b15b",
    "lastUpdateDate": "2025-11-24T04:34:10.632Z",
    "lastBumpType": "minor"
  }
}

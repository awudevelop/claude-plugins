{
  "name": "session",
  "version": "3.8.5",
  "description": "v3.8.5 - Fixed Bash tool parse error in session continue (replaced $() with backticks for pipe compatibility). Status Persistence: Session status now tracked in .auto-capture-state with backward compatibility. Parallel Subagent Architecture (72% token reduction). Zero-blocking conversation logging (<2ms), intelligent analysis at resume (2-4s). Fast, efficient, never lose context!",
  "author": {
    "name": "AutomateWith.Us",
    "email": "team@automatewith.us"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awudevelop/claude-plugins.git"
  },
  "keywords": [
    "session",
    "snapshot",
    "auto-capture",
    "memory",
    "workflow",
    "productivity",
    "optimization",
    "plan-mode",
    "token-reduction",
    "performance",
    "git-history",
    "repository-context",
    "context-tracking"
  ],
  "category": "productivity",
  "license": "MIT",
  "commands": "./commands",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/session-start.js"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/user-prompt-submit.js"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      },
      {
        "matcher": "NotebookEdit",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/stop.js"
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/session-end.js"
          }
        ]
      }
    ]
  },
  "versionMetadata": {
    "lastUpdateCommit": "643bd817ce2f622f12f690076fcd0b089c10d269",
    "lastUpdateDate": "2025-11-17T08:06:02.476Z",
    "lastBumpType": "patch"
  }
}

{
  "name": "session",
  "version": "3.13.0",
  "description": "v3.13.0 - Consolidation Format v2.0: Numbered lists for comprehensive topic capture (95%+ coverage). Anti-recency bias improvements ensure full conversation analysis. Backward compatible with v1.0 format. Planning Feature Phase 1 complete. Parallel Subagent Architecture (72% token reduction). Zero-blocking conversation logging (<2ms), intelligent analysis at resume (2-4s). Fast, efficient, never lose context!",
  "author": {
    "name": "AutomateWith.Us",
    "email": "team@automatewith.us"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/awudevelop/claude-plugins.git"
  },
  "keywords": [
    "session",
    "snapshot",
    "auto-capture",
    "memory",
    "workflow",
    "productivity",
    "optimization",
    "plan-mode",
    "token-reduction",
    "performance",
    "git-history",
    "repository-context",
    "context-tracking"
  ],
  "category": "productivity",
  "license": "MIT",
  "commands": "./commands",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/session-start.js"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": "*",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/user-prompt-submit.js"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      },
      {
        "matcher": "NotebookEdit",
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/post-tool-use.js"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/stop.js"
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "node ${CLAUDE_PLUGIN_ROOT}/hooks/session-end.js"
          }
        ]
      }
    ]
  },
